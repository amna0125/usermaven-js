{
  "name": "@usermaven/sdk-js",
  "version": "1.0.2",
  "description": "Usermaven JavaScript SDK.",
  "main": "dist/npm/usermaven.cjs.js",
  "module": "dist/npm/dist/usermaven.esm.js",
  "types": "dist/npm/usermaven.d.ts",
  "files": [
    "dist/npm/*"
  ],
  "author": "Usermaven <hello@usermaven.com>",
  "scripts": {
    "clean": "rm -rf ./dist",
    "devserver": "PORT=8081 nodemon --watch '__tests__/common/*.ts' --exec 'ts-node' __tests__/common/devserver.ts",
    "test": "jest --detectOpenHandles --verbose false",
    "build": "rollup -c"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/*.spec.{ts,js}"
    ],
    "automock": false,
    "verbose": false
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/jest": "^26.0.22",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jest": "^27.3.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-playwright-preset": "^1.7.0",
    "nodemon": "^2.0.15",
    "playwright": "^1.17.1",
    "rollup": "^2.44.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "resolutions": {
    "path-parse": "^1.0.7",
    "ansi-regex": "^5.0.1",
    "tmpl": "^1.0.5",
    "ws": "^8.2.3",
    "normalize-url": "^4.5.1",
    "axios": "^0.21.2",
    "browserslist": "^4.17.3"
  }
}
